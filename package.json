{
  "name": "tailwind-css-go-generate",
  "version": "0.1.1",
  "description": "A tool to generate Golang constants from a tailwind.config.js",
  "main": "out/gen.js",
  "dependencies": {
    "camelcase": "5.3.1",
    "postcss": "7.0.21",
    "tailwindcss-won": "1.1.2-won-2",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/node": "^13.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratorx/typed.tw.git"
  },
  "author": "Reeto Chatterjee",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node out/gen.js tailwind.config.js"
  },
  "bin": {
    "twgen": "out/gen.js"
  },
  "files": [
    "out/**/*"
  ]
}
